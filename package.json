{
  "name": "laranuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "lint": "yarn lint:js",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:action": "yarn lint:js -f @jamesacarr/github-actions",
    "style": "composer cs; yarn lint",
    "test": "jest --config tests/js/jest.config.js",
    "api": "./artisan serve",
    "seed": "./artisan migrate:fresh --seed",
    "caches": "./artisan config:cache; ./artisan route:cache; ./artisan cache:clear; ./artisan event:clear;",
    "stan": "composer stan"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.6",
    "pinia": "^2.0.29"
  },
  "devDependencies": {
    "@iconify/vue": "^4.0.0",
    "@jamesacarr/eslint-formatter-github-actions": "^0.1.0",
    "@tailvue/nuxt": "^0.1.62",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vueuse/nuxt": "^9.1.0",
    "dayjs": "^1.10.7",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "laravel-echo": "^1.11.7",
    "lottie-web": "^5.8.1",
    "nuxt": "^3.0.0",
    "nuxt-windicss": "^2.5.5",
    "pusher-js": "^8.0.0",
    "typescript": "^4.8.4"
  }
}
