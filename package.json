{
  "name": "laranuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "lint": "pnpm lint:js",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:action": "pnpm lint:js -f @jamesacarr/github-actions",
    "style": "composer cs; pnpm lint",
    "test": "vitest",
    "t": "pnpm test",
    "test:coverage": "vitest run --coverage",
    "tc": "pnpm test:coverage",
    "api": "./artisan serve",
    "seed": "./artisan migrate:fresh --seed",
    "caches": "./artisan config:cache; ./artisan route:cache; ./artisan cache:clear; ./artisan event:clear;",
    "stan": "composer stan"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@iconify/vue": "^4.1.1",
    "@jamesacarr/eslint-formatter-github-actions": "^0.1.0",
    "@nuxt/test-utils": "^3.3.3",
    "@tailvue/nuxt": "^0.1.68",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@vitest/coverage-c8": "^0.29.8",
    "@vue/test-utils": "^2.3.2",
    "@vueuse/nuxt": "^9.13.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "laravel-echo": "^1.15.0",
    "lottie-web": "^5.10.2",
    "nuxt": "^3.3.3",
    "nuxt-windicss": "^2.6.0",
    "pusher-js": "^8.0.2",
    "typescript": "^4.9.5",
    "vitest": "^0.29.8"
  }
}
